        var menu = $(this).closest("li").children(".dpdown-menu");
        if (menu.is(":visible")) {
            menu.slideUp();
        }
        else {
            menu.slideDown();
        }
    });
   var src=document.URL;
	
   src=src.split("/")[src.split("/").length-1];
	src=src.split(".")[0];
   	$("a[href^="+src+"]").addClass("bg-darkGreen").addClass("fg-white");
	$("a[href^="+src+"]").closest(".dpdown-menu").slideDown();
